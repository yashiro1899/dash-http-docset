            <tr><td class="diff_next"></td><td class="diff_header"></td><td nowrap="nowrap">&nbsp;</td><td class="diff_next"></td><td class="diff_header">></td><td nowrap="nowrap"><span class="diff_add">code&gt;GET&nbsp;/api(.*).json&lt;/code&gt;&nbsp;requests&nbsp;without&nbsp;credentials:</span></td></tr>
            <tr><td class="diff_next"></td><td class="diff_header"></td><td nowrap="nowrap"></td><td class="diff_next"></td><td class="diff_header" id="to44_248">248</td><td nowrap="nowrap"><span class="diff_add">&nbsp;&nbsp;&nbsp;&nbsp;&lt;/p&gt;</span></td></tr>
            <tr><td class="diff_next"></td><td class="diff_header"></td><td nowrap="nowrap"></td><td class="diff_next"></td><td class="diff_header" id="to44_249">249</td><td nowrap="nowrap"><span class="diff_add">&nbsp;&nbsp;&nbsp;&nbsp;&lt;pre&gt;</span></td></tr>
            <tr><td class="diff_next"></td><td class="diff_header"></td><td nowrap="nowrap"></td><td class="diff_next"></td><td class="diff_header" id="to44_250">250</td><td nowrap="nowrap"><span class="diff_add">RewriteRule&nbsp;^/api(.*)\.json$&nbsp;/api$1.json&nbsp;[CORS=True]</span></td></tr>
            <tr><td class="diff_next"></td><td class="diff_header"></td><td nowrap="nowrap"></td><td class="diff_next"></td><td class="diff_header" id="to44_251">251</td><td nowrap="nowrap"><span class="diff_add">Header&nbsp;set&nbsp;Access-Control-Allow-Origin&nbsp;"*"&nbsp;env=CORS</span></td></tr>
            <tr><td class="diff_next"></td><td class="diff_header"></td><td nowrap="nowrap"></td><td class="diff_next"></td><td class="diff_header" id="to44_252">252</td><td nowrap="nowrap"><span class="diff_add">Header&nbsp;set&nbsp;Access-Control-Allow-Methods&nbsp;"GET"&nbsp;env=CORS</span></td></tr>
            <tr><td class="diff_next"></td><td class="diff_header"></td><td nowrap="nowrap"></td><td class="diff_next"></td><td class="diff_header" id="to44_253">253</td><td nowrap="nowrap"><span class="diff_add">Header&nbsp;set&nbsp;Access-Control-Allow-Credentials&nbsp;"false"&nbsp;env=CORS</span></td></tr>
            <tr><td class="diff_next"></td><td class="diff_header"></td><td nowrap="nowrap"></td><td class="diff_next"></td><td class="diff_header" id="to44_254">254</td><td nowrap="nowrap"><span class="diff_add">&lt;/pre&gt;</span></td></tr>
